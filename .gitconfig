[user]
	email = fabio.ornellas@gmail.com
	name = Fabio Pugliese Ornellas
[color]
	ui = auto
[http]
	sslVerify = false
[core]
	excludesfile = /home/fornellas/.gitignore
[credential]
	helper = store
[alias]
	aa      = add -A
	br      = branch
	cb      = checkout -b
	cm      = commit
	cma     = commit -a
	co      = checkout
	df      = diff
	ds      = diff --staged
	dump    = cat-file -p
	fuck    = push -f
	lg      = log --graph --oneline --decorate --all
	lga     = log --graph --abbrev-commit --all --pretty=format:'%C(auto)%h %d %s (%Cgreen%an%Creset, %ar)'
	lgs     = log --graph --abbrev-commit --all --pretty=format:'%C(auto)%h %d %s (%Cgreen%an%Creset, %ar)%n' --stat
	pull    = pull --rebase
	st      = status
	type    = cat-file -t
	unstage = reset HEAD
	up      = fetch -p
[push]
	default = simple
